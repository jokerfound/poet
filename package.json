{
  "name": "poetic-journey",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "vercel-build": "chmod +x node_modules/.bin/vite && npm run build",
    "mcp:test": "node src/config/mcpServers.example.js",
    "mcp:health": "node -e \"import('./src/config/mcpServers.js').then(m => m.getMCPServerManager().checkHealth().then(console.log))\"",
    "supabase:test": "node supabase/test-connection.js",
    "supabase:migrate": "node supabase/run-migrations.js",
    "supabase:seed": "node supabase/seed-data.js",
    "supabase:setup": "npm run supabase:migrate && npm run supabase:seed",
    "github:sync": "bash sync-to-github.sh",
    "github:sync:win": "sync-to-github.bat"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.75.0",
    "@vueuse/core": "^10.3.0",
    "axios": "^1.5.0",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.3.4",
    "vite": "^4.4.9"
  }
}