{
  "name": "poet-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "deploy:n8n": "chmod +x deploy-n8n.sh && ./deploy-n8n.sh",
    "start:n8n": "docker-compose -f docker-compose.n8n.yml up -d",
    "stop:n8n": "docker-compose -f docker-compose.n8n.yml down",
    "logs:n8n": "docker logs poet-n8n -f"
  },
  "dependencies": {
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-demi": "^0.14.6",
    "@supabase/supabase-js": "^2.39.7",
    "axios": "^1.6.7"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.5",
    "typescript": "^5.2.0",
    "vue-tsc": "^1.8.25",
    "vite": "^5.2.0"
  }
}